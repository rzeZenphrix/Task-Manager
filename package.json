{
  "name": "task_manager",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-win": "electron-packager . TaskManager --platform=win32 --arch=x64 --out=dist --overwrite --no-prune",
    "package-mac": "electron-packager . TaskManager --platform=darwin --arch=x64 --out=dist --overwrite --no-prune",
    "package": "npm run package-win && npm run package-mac"
  },
  "build": {
    "appId": "com.zenphrix.taskmanager",
    "productName": "Task Manager",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
